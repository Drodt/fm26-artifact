        (rule "deref_assign" (formula "1") (term "1,1")) // ==> {x:=1}{y:=refM<[int]>([[x]])}{y *-> 3}\<{();}\>(true)
        (rule "simple_expr_stmt" (formula "1") (term "1,1,1")) // ==> {x:=1}{y:=refM<[int]>([[x]])}{y *-> 3}\<{}\>(true)
        (rule "sequentialToParallel2" (formula "1")) // ==> {x:=1||{x:=1}(y:=refM<[int]>([[x]]))}{y *-> 3}\<{}\>(true)
        (rule "simplifyUpdate3" (formula "1") (term "1,0")) // ==> {x:=1||y:=refM<[int]>([[x]]))}{y *-> 3}\<{}\>(true)
        (rule "sequentialToParallel2" (formula "1")) // ==> {x:=1||y:=refM<[int]>([[x]]))||{x:=1||y:=refM<[int]>([[x]]))}(y *-> 3)}\<{}\>(true)
        (rule "applyOnMutating" (formula "1") (term "1,0")) // ==> {x:=1||y:=refM<[int]>([[x]]))||{x:=1||y:=refM<[int]>([[x]]))}y *-> {x:=1||y:=refM<[int]>([[x]]))}3)}\<{}\>(true)
        (rule "simplifyUpdate1" (formula "1") (term "1,1,0")) // ==> {x:=1||y:=refM<[int]>([[x]]))||{x:=1||y:=refM<[int]>([[x]]))}y *-> 3)}\<{}\>(true)
