        (rule "let_ident_pat_assign_no_type" (formula "1") (term "1")) // ==> {x:=1}\<{let y; y = &mut x; *y = 3;}\>(true)
        (rule "let_ident_pat_no_type" (formula "1") (term "1")) // ==> {x:=1}\<{y = &mut x; *y = 3;}\>(true)
        (rule "assign_mut_borrow" (formula "1") (term "1")) // ==> {x:=1}{y:=refM<[int]>([[x]])}\<{(); *y = 3;}\>(true)
